      <tr>
        <th>Item</th>
        <th>Manufacture</th>
        <th>Paint Bay</th>
        <th>Painted</th>
        <th>Quality Analysis Bay</th>
        <th>Quality Analysis Check</th>
        <th>Package Bay</th>
      </tr>
    </thead>
    <tbody>
    {{#each var}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.manufacture}}</td>
        <td>{{this.paint}}</td>
        <td>{{this.painted}}</td>
        <td>{{this.qa}}</td>
        <td>{{this.qacheck}}</td>
        <td>{{this.package}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
